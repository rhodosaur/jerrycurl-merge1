<?xml version="1.0"?>
<package xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
        <id>$PackageId$</id>
        <title>$PackageId$</title>
        <version>$PackageVersion$</version>
        <authors>$PackageAuthors$</authors>
        <owners>$PackageOwners$</owners>
        <copyright>$PackageCopyright$</copyright>
        <license type="expression">$PackageLicense$</license>
        <iconUrl>$PackageIconUrl$</iconUrl>
        <projectUrl>$PackageProjectUrl$</projectUrl>
        <tags>$PackageTags$</tags>
        <repository type="git" branch="master" url="$RepositoryUrl$" commit="$GitCommitId$" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>JSON support for Jerrycurl using Newtonsoft.Json.</description>
        <dependencies>
            <group targetFramework="netstandard2.0">
                <dependency id="Newtonsoft.Json" version="$JsonNetPackageVersion$" />
                <dependency id="Jerrycurl.Mvc" version="$PackageVersion$" />
            </group>
            <group targetFramework="netstandard2.1">
                <dependency id="Newtonsoft.Json" version="$JsonNetPackageVersion$" />
                <dependency id="Jerrycurl.Mvc" version="$PackageVersion$" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="$BinPath$**\Jerrycurl.Extensions.Newtonsoft.Json.*" target="lib" />
    </files>
</package>