<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata minClientVersion="3.6">
        <id>$PackageId$</id>
        <title>$PackageId$</title>
        <version>$PackageVersion$</version>
        <authors>$PackageAuthors$</authors>
        <owners>$PackageOwners$</owners>
        <copyright>$PackageCopyright$</copyright>
        <license type="expression">$PackageLicense$</license>
        <icon>$PackageIconFile$</icon>
        <!--<iconUrl>$PackageIconUrl$</iconUrl>-->
        <projectUrl>$PackageProjectUrl$</projectUrl>
        <tags>$PackageTags$</tags>
        <repository type="git" branch="master" url="$RepositoryUrl$" commit="$GitCommitId$" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Jerrycurl CLI. Install as .NET Core Global Tool.</description>
        <packageTypes>
            <packageType name="DotnetTool" />
        </packageTypes>
    </metadata>
    <files>
        <file src="$IconPath$" target="" />
        <file src="$FilesPath$DotnetToolSettings.xml" target="tools\$TargetFramework$\any" />
        <file src="$FilesPath$build.csproj" target="tools\$TargetFramework$\any\build" />
        <file src="$FilesPath$src\**\*" target="tools\$TargetFramework$\any\build" />
        <file src="$BinPath$Jerrycurl.Tools.*" target="tools\$TargetFramework$\any" />
    </files>
</package>